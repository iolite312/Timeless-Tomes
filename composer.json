{
    "name": "dev/eindopdracht",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "authors": [
        {
            "name": "iolite312",
            "email": "68647023+iolite312@users.noreply.github.com"
        }
    ],
    "require": {
        "lcobucci/jwt": "^5.5",
        "ramsey/uuid": "^4.7",
        "lcobucci/clock": "^3.3",
        "rakit/validation": "^1.4",
        "meilisearch/meilisearch-php": "^1.13",
        "guzzlehttp/guzzle": "^7.9",
        "http-interop/http-factory-guzzle": "^1.0",
        "stripe/stripe-php": "^16.5"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.68"
    },
    "scripts": {
        "phpcs:check": "vendor/bin/php-cs-fixer fix --verbose --dry-run --diff",
        "phpcs:fix": "vendor/bin/php-cs-fixer fix"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
